CREATE TABLE FeedDao (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    link TEXT NOT NULL,
    icon BLOB,
    type INTEGER NOT NULL,
    categoryId INTEGER,
    areNotificationsEnabled TEXT NOT NULL
);

getAll:
SELECT * FROM FeedDao;

getById:
SELECT * FROM FeedDao WHERE id = :id;

insert:
INSERT INTO FeedDao(title, link, icon, type, categoryId, areNotificationsEnabled)
VALUES (?, ?, ?, ?, ?, ?);

update:
INSERT OR REPLACE INTO FeedDao
VALUES ?;

insertOrIgnore:
INSERT OR IGNORE INTO FeedDao
VALUES ?;

delete:
DELETE FROM FeedDao WHERE id = :id;

deleteAll:
DELETE FROM FeedDao;